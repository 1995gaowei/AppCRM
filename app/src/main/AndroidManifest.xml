<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ago.appcrm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CustomerListActivity" />
        <activity android:name=".ContactListActivity" />
        <activity android:name=".OpportunityListActivity" />
        <activity android:name=".ContractListActivity" />
        <activity android:name=".ProductListActivity" />
        <activity android:name=".InfoCustomerActivity" />
        <activity android:name=".CustomerDetailActivity" />
        <activity android:name=".InfoContactActivity" />
        <activity android:name=".InfoOpportunityActivity" />
        <activity android:name=".InfoContractActivity" />
        <activity android:name=".OpportunityDetailActivity" />
        <activity android:name=".InfoFollowupActivity" />
        <activity android:name=".InfoProductActivity"></activity>
    </application>

</manifest>